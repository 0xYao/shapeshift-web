{
  "compilerOptions": {
    "target": "es2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "lib": ["ES2021", "DOM", "DOM.Iterable", "WebWorker", "Webworker.Iterable"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "./dist",
    "declaration": true
  },
  "include": [],
  "files": ["./src/sw.ts", "./src/swStub.ts"],
}
